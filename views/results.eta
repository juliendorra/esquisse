<!DOCTYPE html>
<html lang="en">

<head>

  <!-- Generated using https://realfavicongenerator.net/ -->
  <link rel="apple-touch-icon" sizes="180x180" href="/public/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/public/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/public/favicon-16x16.png">
  <link rel="manifest" href="/public/site.webmanifest">
  <meta name="apple-mobile-web-app-title" content="Esquisse">
  <meta name="application-name" content="Esquisse">
  <link rel="shortcut icon" href="/public/favicon.ico">
  <meta name="msapplication-TileColor" content="#603cba">
  <meta name="msapplication-config" content="/public/browserconfig.xml">
  <meta name="theme-color" content="#ffffff">

  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <title>Your generated results Â· Esquisse AI</title>

  <style>
    :root {
      --footer-height: 3rem;
      --scale: 0.4;

      --neutral-color: #FaFaFa;
      --neutral-color-highlight: #fefefe;
      --light-border: #E0E0E0;
      --title-text-color: #929292;
      --footer-btn-color: #363636;

      --slight-passthrough-color: rgba(245, 245, 245, 0.5);

      --group-break-color-code-button: #929292;
      --group-break-color-code-border: #ffffff;

      --group-static-color-code: rgb(154, 123, 212);

      --group-text-color-code: rgb(186, 226, 127);
      --group-image-color-code: rgb(233, 107, 168);

      --card-shadow: 0 0 20px rgba(255, 255, 255, 0.3);
      --primary-font: 'Helvetica Neue', sans-serif;

      --apps-app-title-link-color: rgb(93, 64, 147);
      --apps-background-color: rgb(223, 167, 253);

    }

    html {
      font-family: var(--primary-font);
      box-sizing: border-box;
    }

    body {
      position: relative;
      margin: 0;
      padding: 0;
      font-weight: normal;
      background-color: var(--apps-background-color);
    }

    :not(:defined) {
      visibility: hidden;
    }

    .results-list-container {
      padding: 3em 7vw;
    }

    .results-page-title {
      color: white;
    }

    .results-list {
      list-style-type: none;
      display: flex;
      flex-wrap: wrap;
      padding: 0;
    }

    .results-list li {
      background-color: white;
      border-radius: 2em;
      padding: 0;
      height: min-content;
      width: min-content;
      box-sizing: border-box;
      margin-bottom: 1em;
      margin-left: 1em;
    }

    @media (max-width: 600px) {
      .results-list-container {
        padding: 0.5em;
      }

      .results-list li {
        padding: 1em;
      }
    }

    .results-list .app-name {
      margin-bottom: 0.8em;
    }

    .results-list a {
      text-decoration: none;
      color: var(--apps-app-title-link-color);
      display: flex;
      flex-direction: column;
      justify-content: space-between;
      height: 28em;
      padding: 0.8em;
    }

    .result-item {
      position: relative;
      display: inline-block;
      margin: 1.5em;
      width: min-content;
    }

    .result-info {
      padding: 0.5em;
    }

    .result-name {
      font-weight: bold;
      font-size: 1.1em;
      color: var(--apps-app-title-link-color);
      margin: 0.5em;
      text-align: left;
      line-height: 1.3em;
      max-height: calc(2*1.3em);
      overflow: hidden;
    }

    .result-snippet {
      font-size: 0.9em;
      color: var(--title-text-color);
      margin-left: 0.5em;
      margin-right: 0.5em;
      line-height: 1.4;
    }

    .result-snippet-placeholder span {
      display: block;
      position: relative;
      height: 0.3em;
      background-color: #ededed;
      border-radius: 0.4em;
      margin-top: 1em;
      margin-bottom: 0.5em;
      width: 80%;
      margin-left: 0.5em;
      margin-right: 0.5em;
    }

    .result-snippet-placeholder span:nth-last-child(2n+1) {
      width: 60%;
      margin-bottom: 1em;
    }

    .result-snippet-placeholder span:last-child {
      width: 30%;
      margin-bottom: 0;
    }

    .result-preview {
      background-image: url('/icons/placeholder-gradient.jpg');
      background-size: cover;
      background-position: center;
      border-radius: 0 0 1.75em 1.75em;
      padding: 0;
      margin-left: 0;
      margin-right: 0;
      width: 16em;
      height: 16em;
    }
  </style>

  <script src="/results-page.js" type="module" async></script>

</head>

<body>
  <div class="results-list-container">

    <h2 class="results-page-title">Your generated results</h2>

    <!-- Render Today results -->
    <% if (it.categorizedResults.today.length) { %>
    <h3 class="results-page-title">Today</h3>
    <ul class="results-list">
      <% it.categorizedResults.today.forEach(function(result){ %>
      <li class="result-item">
        <a target="_blank" href="/result/<%= result.resultid %>">
          <div class="result-info">
            <% if (result.generatedtitle) { %>
            <div class="result-name"><%= result.generatedtitle %></div>
            <% } else if (result.name) { %>
            <div class="result-name"><%= result.name %></div>
            <% } %>

            <% if (result.generatedsummary) { %>
            <div class="result-snippet"><%= result.generatedsummary %></div>
            <% } else if (result.snippet) { %>
            <div class="result-snippet"><%= result.snippet %></div>
            <% } else { %>
            <div class="result-snippet-placeholder"><span></span><span></span><span></span><span></span></div>
            <% } %>
          </div>
          <img src="/thumbnail/<%= result.resultid %>" class="result-preview">
        </a>
      </li>
      <% }) %>
    </ul>
    <% } %>

    <!-- Render Yesterday results -->
    <% if (it.categorizedResults.yesterday.length) { %>
    <h3 class="results-page-title">Yesterday</h3>
    <ul class="results-list">
      <% it.categorizedResults.yesterday.forEach(function(result){ %>
      <li class="result-item">
        <a target="_blank" href="/result/<%= result.resultid %>">
          <% if (result.name || result.snippet) { %>
          <div class="result-info">
            <% if (result.generatedtitle) { %>
            <div class="result-name"><%= result.generatedtitle %></div>
            <% } else if (result.name) { %>
            <div class="result-name"><%= result.name %></div>
            <% } %>

            <% if (result.generatedsummary) { %>
            <div class="result-snippet"><%= result.generatedsummary %></div>
            <% } else if (result.snippet) { %>
            <div class="result-snippet"><%= result.snippet %></div>
            <% } else { %>
            <div class="result-snippet-placeholder"><span></span><span></span><span></span><span></span></div>
            <% } %>
          </div>
          <% } %>
          <img src="/thumbnail/<%= result.resultid %>" class="result-preview">
        </a>
      </li>
      <% }) %>
    </ul>
    <% } %>

    <!-- Render results from this month -->
    <% if (it.categorizedResults.currentMonth.length) { %>
    <h3 class="results-page-title"><%= it.categorizedResults.currentMonth[0].formattedDate %></h3>
    <ul class="results-list">
      <% it.categorizedResults.currentMonth.forEach(function(result){ %>
      <li class="result-item">
        <a target="_blank" href="/result/<%= result.resultid %>">
          <% if (result.name || result.snippet) { %>
          <div class="result-info">
            <% if (result.generatedtitle) { %>
            <div class="result-name"><%= result.generatedtitle %></div>
            <% } else if (result.name) { %>
            <div class="result-name"><%= result.name %></div>
            <% } %>

            <% if (result.generatedsummary) { %>
            <div class="result-snippet"><%= result.generatedsummary %></div>
            <% } else if (result.snippet) { %>
            <div class="result-snippet"><%= result.snippet %></div>
            <% } else { %>
            <div class="result-snippet-placeholder"><span></span><span></span><span></span><span></span></div>
            <% } %>
          </div>
          <% } %>
          <img src="/thumbnail/<%= result.resultid %>" class="result-preview">
        </a>
      </li>
      <% }) %>
    </ul>
    <% } %>

    <!-- Render previous months' results -->
    <% Object.keys(it.categorizedResults.previousMonths).forEach(function(month) { %>
    <h3 class="results-page-title"><%= month %></h3>
    <ul class="results-list">
      <% it.categorizedResults.previousMonths[month].forEach(function(result){ %>
      <li class="result-item">
        <a target="_blank" href="/result/<%= result.resultid %>">
          <% if (result.name || result.snippet) { %>
          <div class="result-info">
            <% if (result.generatedtitle) { %>
            <div class="result-name"><%= result.generatedtitle %></div>
            <% } else if (result.name) { %>
            <div class="result-name"><%= result.name %></div>
            <% } %>

            <% if (result.generatedsummary) { %>
            <div class="result-snippet"><%= result.generatedsummary %></div>
            <% } else if (result.snippet) { %>
            <div class="result-snippet"><%= result.snippet %></div>
            <% } else { %>
            <div class="result-snippet-placeholder"><span></span><span></span><span></span><span></span></div>
            <% } %>
          </div>
          <% } %>
          <img src="/thumbnail/<%= result.resultid %>" class="result-preview">
        </a>
      </li>
      <% }) %>
    </ul>
    <% }) %>

    <!-- Render results from last year -->
    <% if (it.categorizedResults.lastYear.length) { %>
    <h3 class="results-page-title">2023</h3>
    <ul class="results-list">
      <% it.categorizedResults.lastYear.forEach(function(result){ %>
      <li class="result-item">
        <a target="_blank" href="/result/<%= result.resultid %>">
          <% if (result.name || result.snippet) { %>
          <div class="result-info">
            <% if (result.generatedtitle) { %>
            <div class="result-name"><%= result.generatedtitle %></div>
            <% } else if (result.name) { %>
            <div class="result-name"><%= result.name %></div>
            <% } %>

            <% if (result.generatedsummary) { %>
            <div class="result-snippet"><%= result.generatedsummary %></div>
            <% } else if (result.snippet) { %>
            <div class="result-snippet"><%= result.snippet %></div>
            <% } else { %>
            <div class="result-snippet-placeholder"><span></span><span></span><span></span><span></span></div>
            <% } %>
          </div>
          <% } %>
          <img src="/thumbnail/<%= result.resultid %>" class="result-preview">
        </a>
      </li>
      <% }) %>
    </ul>
    <% } %>
  </div>

</body>

</html>